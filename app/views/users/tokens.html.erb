<% cache ['v1', @tokens] do %>
    <%= content_for :page_title, t('users.title.tokens') %>
    <%= content_for :nav, render('dropdown_menu') %>
    <% @tokens.each do |token| %>
        <%= render layout: 'layouts/list_group' do %>
            <%= render 'tokens/controls', token: token %>
            <%= render 'tokens/token', token: token %>
        <% end %>
    <% end %>
<% end %>
