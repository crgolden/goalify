  gem 'rspec-rails'
  gem 'selenium-webdriver'
  gem 'shoulda-matchers'
  gem 'simplecov', require: false
  gem 'spring' # Spring speeds up development by keeping your application running in the background.
  gem 'spring-commands-rspec'
  gem 'travis'
