<% if user_signed_in? %>
    <li><%= link_to(edit_user_registration_path) do %>
          <i class="glyphicon glyphicon-edit"></i> Edit registration
      <% end %></li>
    <li class="divider"></li>
<% else %>
    <li><%= link_to(new_user_registration_path) do %>
          <i class="glyphicon glyphicon-edit"></i> Register
      <% end %></li>
    <li class="divider"></li>
<% end %>