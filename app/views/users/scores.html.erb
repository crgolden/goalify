<% cache ['v1', @scores] do %>
    <%= content_for :page_title, t('users.title.scores') %>
    <%= content_for :nav, render('dropdown_menu') %>
    <% @scores.each do |score| %>
        <%= render layout: 'layouts/list_group' do %>
            <%= render 'scores/score', score: score %>
        <% end %>
    <% end %>
<% end %>
