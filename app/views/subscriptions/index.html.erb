<%= content_for :title, t('goals.title.index') %>
<% if @subscriptions.present? %>
    <%= render layout: 'layouts/panel', locals: {title: 'Viewing Goal Subscriptions', resource: nil} do %>
        <%= render 'goals/subscriptions' %>
    <% end %>
<% else %>
    <% flash[:notice] = 'Create a subscription for this Goal!' %>
<% end %>
